@model IEnumerable<Basketball.Domain.Entities.Faq>

@{
    ViewBag.Title = "Faqs";
}

<h1>Faqs</h1>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table cellpadding="0" cellspacing="0" class="leagueTableHome">
    <tr>
        <th>
            Title
        </th>
        <th>Last Updated</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
			@item.Title
        </td>
        <td>
			@item.LastUpdated.ToShortDateString()
        </td>
        <td class="textRight">
            @Html.ActionLink("Edit", "Edit", new { id=item.Id })
        </td>
    </tr>
}

</table>


